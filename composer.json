{
    "name": "detailnet/tcpdf-extend-annotations",
    "type": "library",
    "description": "Fork of TCPDF to support line,polyline and ink annotation positioning.",
    "keywords": [
        "PDF",
        "tcpdf",
        "PDFD32000-2008",
        "qrcode",
        "datamatrix",
        "pdf417",
        "barcodes",
        "tecnickcom/tcpdf",
        "anotations"
    ],
    "homepage": "http://www.tcpdf.org/",
    "version": "6.8.2",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Nicola Asuni",
            "email": "info@tecnick.com",
            "role": "lead"
        },
        {
            "name": "Curzio Della Santa",
            "email": "cdellasanta@detail.ch"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "ext-curl": "*"
    },
    "autoload": {
        "classmap": [
            "config",
            "include",
            "tcpdf.php",
            "tcpdf_parser.php",
            "tcpdf_import.php",
            "tcpdf_barcodes_1d.php",
            "tcpdf_barcodes_2d.php",
            "include/tcpdf_colors.php",
            "include/tcpdf_filters.php",
            "include/tcpdf_font_data.php",
            "include/tcpdf_fonts.php",
            "include/tcpdf_images.php",
            "include/tcpdf_static.php",
            "include/barcodes/datamatrix.php",
            "include/barcodes/pdf417.php",
            "include/barcodes/qrcode.php"
        ]
    },
    "archive": {
        "exclude": [
            "/examples"
        ]
    }
}
